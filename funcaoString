public class Main {
	public static void main(String[] args) {

		String s = "potato apple lemon orange";
		
		String[] vect = s.split(" "); // A função split vai separar toda palavra separada por espaço e criar um vetor
		String word1 = vect[0];       // que poderá ser acessado posteriormente usando seu index.
		String word2 = vect[1];	     
		String word3 = vect[2];
		
		System.out.println(vect[0]);
		System.out.println(vect[1]);
		System.out.println(vect[2]);
		System.out.println(vect[3]);
	}
}
